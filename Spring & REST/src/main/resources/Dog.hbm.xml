<hibernate-mapping package="com.epam.javacource.sid.spring.model">
    <class name="Dog" table="dog">
        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>
        <property name="name" type="string" column="name"/>
        <property name="dateOfBirth" type="string" column="date_of_birth"/>
        <property name="height" type="long" column="height"/>
        <property name="weight" type="long" column="weight"/>
    </class>
</hibernate-mapping>